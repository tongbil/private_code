<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qixin.ssh.model.MailAccept" table="mailaccept">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>		
		<property name="emilName" type="java.lang.String">
			<column name="emilName" />
		</property>
		<property name="emilPassword" type="java.lang.String">
			<column name="emilPassword" />
		</property>
		<property name="server" type="java.lang.String">
			<column name="server" />
		</property>
		<property name="port" type="java.lang.String">
			<column name="port" />
		</property>
	</class>
</hibernate-mapping>  