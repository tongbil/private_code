<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qixin.ssh.model.Semail" table="data_semail">
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="fName" type="java.lang.String">
			<column name="fName" />
		</property>	
		
		<property name="sName" type="java.lang.String">
			<column name="sName" />
		</property>	
		<property name="status" type="java.lang.String">
			<column name="status" />
		</property>	
		<property name="content" type="java.lang.String">
			<column name="content" />
		</property>	
		<property name="time" type="java.lang.String">
			<column name="time" />
		</property>
		<property name="title" type="java.lang.String">
			<column name="title" />
		</property>
		<property name="reader" type="java.lang.String">
			<column name="reader" />
		</property>
		<property name="cName" type="java.lang.String">
			<column name="cName" />
		</property>
		<property name="delWhy" type="java.lang.String">
			<column name="delWhy" />
		</property>
		<property name="sR" type="java.lang.String">
			<column name="sR" />
		</property>
		<property name="reportId" type="java.lang.String">
			<column name="reportId" />
		</property>
		<property name="srTime" type="java.lang.String">
			<column name="srTime" />
		</property>
		<property name="srUser" type="java.lang.String">
			<column name="srUser" />
		</property>
		<property name="sjTime" type="java.lang.String">
			<column name="sjTime" />
		</property>
		<property name="sjUser" type="java.lang.String">
			<column name="sjUser" />
		</property>
		<property name="ljUser" type="java.lang.String">
			<column name="ljUser" />
		</property>
		<property name="bgCode" type="java.lang.String">
			<column name="bgCode" />
		</property>
		

	</class>
</hibernate-mapping>  