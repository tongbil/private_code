<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qixin.ssh.model.Numpro" table="data_number">
		<id name="numProId" type="int">
			<column name="numProId" />
		
			<generator class="assigned" />
		</id>
		<property name="productId" type="int">
			<column name="productId" />
		</property>	
		<property name="produName" type="java.lang.String">
			<column name="produName" />
		</property>	
		<property name="number" type="int">
			<column name="number" />
		</property>	
	</class>
</hibernate-mapping>  