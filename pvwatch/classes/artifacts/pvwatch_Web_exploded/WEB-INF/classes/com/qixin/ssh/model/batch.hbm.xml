<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qixin.ssh.model.Batch" table="data_batch">
		<id name="batchId" type="int">
			<column name="batchId" />
			<generator class="native" />
		</id>
		<property name="batchNum" type="java.lang.String">
			<column name="batchNum" />
		</property>	
		<property name="productName" type="java.lang.String">
			<column name="productName" />
		</property>	
		<property name="CFDACode" type="java.lang.String">
			<column name="CFDACode" />
		</property>	
		<property name="dosage" type="java.lang.String">
			<column name="dosage" />
		</property>	
		<property name="strength" type="java.lang.String">
			<column name="strength" />
		</property>	
		<property name="manufactureTime" type="java.util.Date">
			<column name="manufactureTime" />
		</property>	
		<property name="number" type="int">
			<column name="number" />
		</property>	
		<property name="packaging" type="java.lang.String">
			<column name="packaging" />
		</property>	
		<property name="Manufacturer" type="java.lang.String">
			<column name="Manufacturer" />
		</property>	
		<property name="productId" type="int">
			<column name="productId" />
		</property>	
		<property name="mahId" type="java.lang.String">
			<column name="mahId" />
		</property>	
	</class>
</hibernate-mapping>  