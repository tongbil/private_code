<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qixin.ssh.model.Sales" table="data_sales">
		<id name="presId" type="int">
			<column name="presId" />
			<generator class="assigned" />
		</id>
		<property name="productId" type="java.lang.String">
			<column name="productId" />
		</property>	
		<property name="proName" type="java.lang.String">
			<column name="proName" />
		</property>	
		<property name="CFDACode" type="java.lang.String">
			<column name="CFDACode" />
		</property>	
		<property name="numbers" type="java.lang.String">
			<column name="numbers" />
		</property>	
		<property name="adress" type="java.lang.String">
			<column name="adress" />
		</property>	
		<property name="intime" type="java.util.Date">
			<column name="intime" />
		</property>	
		<property name="outtime" type="java.util.Date">
			<column name="outtime" />
		</property>	
		<property name="strength" type="java.lang.String">
			<column name="strength" />
		</property>	
		<property name="danwei" type="java.lang.String">
			<column name="danwei" />
		</property>	
		
	</class>
</hibernate-mapping>  