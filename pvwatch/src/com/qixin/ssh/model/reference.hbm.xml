<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.qixin.ssh.model.Reference" table="iscr_product_medical_reference">
		<id name="id" type="int">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<property name="productName" type="java.lang.String">
			<column name="productName" />
		</property>	
		<property name="eventName" type="java.lang.String">
			<column name="eventName" />
		</property>	
		<property name="eventCode" type="java.lang.String">
			<column name="eventCode" />
		</property>	
		<property name="userId" type="int">
			<column name="userId" />
		</property>	
		<property name="content" type="java.lang.String">
			<column name="content" />
		</property>	
		<property name="createTime" type="java.util.Date">
			<column name="createTime" />
		</property>	
		<property name="element" type="java.lang.String">
			<column name="element" />
		</property>	
	</class>
</hibernate-mapping>  